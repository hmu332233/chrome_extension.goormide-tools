{
  "manifest_version": 2,
  "name": "goormide-tools",
  "version": "0.0.2",
  "description": "goormide-tools",
  "browser_action": {
    "default_title": "goormide-tools"
  },
  "content_scripts": [
    {
      "matches": ["https://ide-run.goorm.io/*", "https://ide-beta-run.goorm.io/*"],
      "js": ["build/contentScripts.js"],
      "css": ["build/style.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["build/app.js"],
  "permissions": ["activeTab"]
}